// Online C++ compiler to run C++ program online
#include <iostream>
int loop_num = 0;
int fixed_loop;
void printMyname() {
    std::cout << "Supanat" << std::endl ;
}
void getLoop() {
    std::cin >> fixed_loop;
    
}

int main() {
    getLoop();
    while(loop_num <= fixed_loop){
        std::cout << "loop " << loop_num ;
        if(loop_num == 7){
            std::cout << "Ratchapol najaaa" << std::endl;
        }else{
            printMyname();
        }
        loop_num++;
    }
    return 0;
}
